<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	
	
	<package name="trainingvenue"  extends="struts-default" namespace="/trainingvenue">
		<!-- 初始化查询页面 -->
		<action name="initSearchtra" class="trainingvenueAction" method="initSearch">
			<result name="success">/page/lab/UndergraduateLab-in-list.jsp</result>
			<result name="errorstring">../errorstring.jsp</result>
		</action>
		<!--查询专利信息  -->
		<action name="findTrainingvenue" class="trainingvenueAction" method="findTrainingvenue">
			<result name="success">/page/lab/UndergraduateLab-in-list.jsp</result>
			<result name="errorstring">../errorstring.jsp</result>
		</action>
		<!--跳转到修改页面-->
		<action name="editToTrainingvenue" class="trainingvenueAction" method="initEdit">
			<result name="success">/page/lab/UndergraduateLab-in-edit.jsp</result>
		</action>
		<!--修改-->
		<action name="editTrainingvenue" class="trainingvenueAction" method="modifiTrainingvenue">
			<result name="success" type="redirectAction">initSearchtra</result>
		</action>
		<!--删除-->
		<action name="deleteTrainingvenue" class="trainingvenueAction" method="deleteTrainingvenue">
			<result name="success" type="redirectAction">initSearchtra</result>
		</action>
		<!--查看-->
		<action name="editToE" class="trainingvenueAction" method="initEquipment">
			<result name="success">/page/lab/Equipment.jsp</result>
		</action>
		<!--设备-->
		<action name="editE" class="trainingvenueAction" method="modifiEquipment">
			<result name="success" type="redirectAction">initSearch</result>
		</action>
		<!-- 赛项查询 -->
		<action name="searchContestToTCO" class="trainingvenueAction"
			method="findTrainingvenue">
			<result name="success">/page/lab/UndergraduateLab-in-list.jsp</result>
		</action>
		 <!-- 导出 -->
        <action name="exportSubContestTExcel1" class="exportAction" method="export">
        	<result name="success" type="redirectAction">findTrainingvenue.action</result>
        </action>
         <!-- 导入 -->
		<action name="attachFileList" class="importAction" method="attachFileList">
			<result name="success">/page/common/import.jsp</result>
			<result name="error">/page/common/importerror.jsp</result>
		</action>
		<action name="upDown_uploadFile" class="importAction" method="importExcel">
    		<result name="success">/page/common/importsuccess.jsp</result>
    		<result name="majorerror">/page/common/importmajorerror.jsp</result>
    		<result name="majorStyleerror">/page/common/importStyleerror.jsp</result>
    		<result name="error">/page/common/importerror.jsp</result>
    	</action>
	</package>
</struts>    
